{
    "imports": {
        "@hono/hono": "jsr:@hono/hono@4.7.11",
        "@hono/swagger-ui": "npm:@hono/swagger-ui@0.5.1",
        "@hono/zod-openapi": "npm:@hono/zod-openapi@0.19.8",
        "@std/assert": "jsr:@std/assert@1.0.13",
        "@std/fs": "jsr:@std/fs@1.0.18",
        "@std/path": "jsr:@std/path@1.1.0",
        "@supabase/supabase-js": "jsr:@supabase/supabase-js@2.49.10",
        "async-mutex": "npm:async-mutex@0.5.0",
        "bcrypt-ts": "npm:bcrypt-ts@7.0.0",
        "flat": "npm:flat@6.0.1",
        "hono": "npm:hono@4.7.11",
        "hono-i18n": "npm:hono-i18n@1.1.1",
        "http-status-code": "npm:http-status-code@2.1.0",
        "neverthrow": "npm:neverthrow@8.2.0",
        "zod": "npm:zod@3.25.53",
        "zod-openapi": "npm:zod-openapi@4.2.4",
        "@util/": "./util/",
        "@routes/": "./routes/",
        "@controllers/": "./controllers/",
        "@middleware/": "./middleware/",
        "@db/": "./db/",
        "@i18n/": "./i18n/",
        "@define/": "./define/",
        "@app/": "./app/",
        "@data/": "./data/"
    },
    "tasks": {
        "start": "deno run --allow-net main.ts"
    },
    "compilerOptions": {
        "jsx": "precompile",
        "jsxImportSource": "hono/jsx",
        "verbatimModuleSyntax": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true
    },
    "deploy": {
        "project": "exam-nexus",
        "exclude": [
            "**/node_modules",
            "./deno.lock"
        ],
        "include": [],
        "entrypoint": "main.ts"
    },
    "fmt": {
        "indentWidth": 4,
        "useTabs": false,
        "lineWidth": 220
    }
}
